<%- include('header'); -%>
  <div class="box" id="heading">
    <h1><%= listTitle %> todo list</h1>

  </div>
  <div class="box">
    <% for (var i =0; i < newItems.length; i++){%>
      <form action="/delete" method="post">
       <div class="item">
      <input type="checkbox" value="<%= newItems[i].id  %> " onChange="this.form.submit()" name="checkBox">
      <% console.log(newItems[i].name);%>
      <p> <%=  newItems[i].name %> </p>
    </div>
  </form>
    <%}%>
    <form class="item" action="/" method="post">
      <input type="text" name="userInput" placeholder="New Item" autocomplete="off" >
      <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
</div>
<%- include('footer'); -%>
